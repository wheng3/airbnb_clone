<%= form_for :listing, url: {controller: 'listings', action: 'create'} do |f| %>
  Name			: <%= f.text_field :name, type: 'string' %><br />
  Bed Number	: <%= f.text_field :bed_number, type: 'integer' %><br />
  Guest Number	: <%= f.text_field :guest_number, type: 'integer' %><br />
  Address		: <%= f.text_field :address, type: 'string' %><br />
  Price			: <%= f.text_field :price, type: 'decimal' %><br />
  Description   : <%= f.text_area  :description, type: 'text' %><br />
  Photos		: <%= f.file_field :photos, multiple: true %><br />
  <%= f.submit %>
<% end %>